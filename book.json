{
  "root": "docs",
  "plugins": [
    "heading-anchors",
    "include-codeblock",
    "edit-link",
    "hints",
    "favicon",
    "-fontsettings"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/qooxdoo/docs/edit/master",
      "label": "Edit this page",
      "placement": "left"
    },
    "sharing": {
      "facebook": false,
      "twitter": false
    },
    "favicon":{
        "shortcut": "assets/favicon.png",
        "bookmark": "assets/favicon.png"
    }
  }
}
